generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model StockEntry {
  id        String   @id @default(uuid())
  productId String
  quantity  Int
  createdAt DateTime @default(now())
}

model StockExit {
  id        String   @id @default(uuid())
  productId String
  quantity  Int
  createdAt DateTime @default(now())
}
